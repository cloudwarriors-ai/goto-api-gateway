# GoTo API Gateway Environment Configuration Template
# Copy this file to .env and fill in your actual values

# OAuth Client Credentials (required)
# Get these from your GoTo Developer account at https://developer.goto.com
CLIENT_ID=your_client_id_here
CLIENT_SECRET=your_client_secret_here

# OAuth Redirect URI (required)
# This should match what you configured in your GoTo app settings
REDIRECT_URI=http://localhost:9111

# Login Credentials for Automated OAuth (required for automated login)
# These are your GoTo account credentials for automated browser authentication
LOGIN_EMAIL=your_goto_email@example.com
LOGIN_PASSWORD=your_goto_password

# Access Tokens (automatically managed - leave blank initially)
# These will be automatically populated after successful OAuth authentication
ACCESS_TOKEN=
REFRESH_TOKEN=

# Voice Admin Tokens (automatically managed - leave blank initially)  
# These will be automatically populated after successful Voice API OAuth authentication
VOICE_ACCESS_TOKEN=
VOICE_REFRESH_TOKEN=

# Redis Configuration (required for session management)
# Redis connection settings for storing credentials and sessions
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Logging Configuration (optional)
# Set log level: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# MCP Auth Server Configuration
MCP_AUTH_PORT=9112
MCP_AUTH_HOST=localhost